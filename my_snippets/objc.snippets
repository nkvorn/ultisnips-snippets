
snippet method "- (...) ....: ..." b
- (${1:void})${2:methodName};$0
endsnippet

snippet imethod "- (...) ... {  ... }" b
- (${1:void})${2:methodName}
{
    $0
}
endsnippet

snippet ai "alloc + init" b
${1:var} = [[${2:type} alloc] init];$0
endsnippet

snippet iss "alloc + init" b
NS${1:Mutable}String *${2:str} = [[NS$1String alloc] init];
$0
endsnippet

snippet fori "for" b
for (int ${1:i} = 0; $1 < ${2:10}; $1++)`!p nl(snip)`{
	$0
}
endsnippet
